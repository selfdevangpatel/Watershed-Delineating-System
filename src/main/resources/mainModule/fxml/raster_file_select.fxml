<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="50.0" layoutY="50.0" text="Raster file path:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="rasFilePathTF" layoutX="165.0" layoutY="48.0" prefWidth="420.0" />
      <Button fx:id="rasFileSelBtn" layoutX="595.0" layoutY="48.0" mnemonicParsing="false" text="..." />
      <Button fx:id="rasAddBtn" layoutX="165.0" layoutY="140.0" mnemonicParsing="false" text="Add" />
      <Label fx:id="successLB" layoutX="215.0" layoutY="142.0" text="Added to map." textFill="#00c800" visible="false">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="unsuccessfulLB" layoutX="215.0" layoutY="142.0" text="Failed to add to map." textFill="RED" visible="false">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
